{
  "manifest_version": 3,
  "name": "Laravext Page Data",
  "version": "1.0",
  "description": "Displays JSON from window.__laravext in a custom DevTools panel",
  "permissions": [
    "activeTab",
    "storage"
  ],
  "background": {
    "service_worker": "background.js"
  },
  "icons": {
    "128": "icons/128.png"
  },
  "devtools_page": "devtools.html"
}
