{
    "name": "@laravext/react",
    "version": "0.0.5",
    "description": "Laravext for React",
    "homepage": "https://laravext.dev/",
    "repository": {
        "type": "git",
        "url": "https://github.com/arthurydalgo/laravext",
        "directory": "npm/react"
    },
    "main": "./dist/cjs/index.cjs.js",
    "module": "./dist/esm/index.esm.js",
    "exports": {
        ".": {
            "import": "./dist/esm/index.esm.js",
            "require": "./dist/cjs/index.cjs.js"
        },
        "./server": {
            "import": "./dist/esm/server.esm.js",
            "require": "./dist/cjs/server.cjs.js"
        },
        "./tools": {
            "import": "./dist/esm/tools.esm.js",
            "require": "./dist/cjs/tools.cjs.js"
        },
        "./router": {
            "import": "./dist/esm/router.esm.js",
            "require": "./dist/cjs/router.cjs.js"
        },
        "./progress": {
            "import": "./dist/esm/progress.esm.js",
            "require": "./dist/cjs/progress.cjs.js"
        }
    },
    "private": false,
    "scripts": {
        "build": "rollup -c",
        "test": "echo \"Error: no test specified\" && exit 1"
    },
    "author": "Arthur Ydalgo",
    "license": "ISC",
    "dependencies": {
        "axios": "^1.7.4",
        "jsdom": "^24.1.0",
        "nprogress": "^0.2.0",
        "react": "^18.2.0",
        "react-dom": "^18.2.0"
    },
    "devDependencies": {
        "@babel/core": "^7.20.0",
        "@babel/preset-env": "^7.20.0",
        "@babel/preset-react": "^7.20.0",
        "@rollup/plugin-commonjs": "^21.0.0",
        "@rollup/plugin-json": "^4.1.0",
        "@rollup/plugin-node-resolve": "^13.0.0",
        "@rollup/plugin-terser": "^0.4.4",
        "rollup-plugin-babel": "^4.4.0",
        "rollup-plugin-terser": "^7.0.2"
    }
}
