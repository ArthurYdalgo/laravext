{
  "name": "arthurydalgo/laravext",
  "description": "",
  "keywords": [
    "arthurydalgo",
    "laravext"
  ],
  "homepage": "https://github.com/arthurydalgo/laravext",
  "license": "MIT",
  "type": "library",
  "authors": [
    {
      "name": "Arthur Ydalgo",
      "email": "arthur.ydalgo.dev@gmail.com",
      "role": "Developer"
    }
  ],
  "require": {
    "php": "^7.4|^8.0",
    "illuminate/support": "^8.0"
  },
  "require-dev": {
    "orchestra/testbench": "^6.0",
    "phpunit/phpunit": "^9.0"
  },
  "autoload": {
    "psr-4": {
      "ArthurYdalgo\\Laravext\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "ArthurYdalgo\\Laravext\\Tests\\": "tests"
    }
  },
  "scripts": {
    "test": "vendor/bin/phpunit",
    "test-coverage": "vendor/bin/phpunit --coverage-html coverage"
  },
  "config": {
    "sort-packages": true
  },
  "extra": {
    "laravel": {
      "providers": [
        "ArthurYdalgo\\Laravext\\LaravextServiceProvider"
      ],
      "aliases": {
        "Laravext": "ArthurYdalgo\\Laravext\\LaravextFacade"
      }
    }
  }
}
